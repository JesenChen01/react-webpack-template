{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve:dev": "cross-env NODE_ENV=dev webpack serve --config ./webpack/webpack.dev.js",
    "serve:prod": "cross-env NODE_ENV=prod webpack serve --config ./webpack/webpack.dev.js",
    "build:dev": "cross-env NODE_ENV=dev webpack --config ./webpack/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=prod webpack --config ./webpack/webpack.prod.js",
    "lint:eslint": "eslint --config .eslintrc.js src --fix",
    "lint:prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,css,scss,less,md,vue,html}\"",
    "lint:prettier:check": "prettier --check \"**/*.{ts,tsx,js,jsx,css,scss,less,md,vue,html}\"",
    "lint:style": "stylelint --config .stylelintrc.js \"./**/*.{css,scss,less}\" --fix",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "JesenChen",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "path": "^0.12.7",
    "postcss-scss": "^4.0.6",
    "prettier": "3.0.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --config .eslintrc.js --fix"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ],
    "*.{css,less,scss}": [
      "stylelint --fix"
    ]
  }
}
